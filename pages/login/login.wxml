<!-- 登陆页面 start -->
<view class='loginWrap' wx:if="{{!login}}">
    <image src='{{title_img}}'></image>

    <form bindsubmit="formSubmit">
        <view><text>账号</text><input type='tel' name="user" placeholder='请输入注册的手机号'></input></view>
        <view><text>密码</text><input type='password' name="password" placeholder='请输入密码'></input></view>

        <button formType="submit">登 录</button>
    </form>
    <view class='others'>
        <navigator url='../register/register'>立即注册</navigator>
        <navigator url='../forgetPassword/forgetPassword'>忘记密码</navigator>
    </view>
</view>
<!-- 登陆页面 end -->